import random
def create_adjancency_matrix(V, E):
    matrix = [[0]*V for _ in range(V)]
    for edge in E:
        u,v = edge
        randomnumber = random.randint(0,9)
        matrix[u][v] = randomnumber
        matrix[v][u] = randomnumber
    return matrix

vertices = 6
edges = [(0, 1), (1, 2), (1, 3), (2, 3), (3, 0), (3,2),(3,5),(4,0),(4,2),(5,2),(5,4)]
matrix = create_adjancency_matrix(vertices,edges)
for i in range (0,len(matrix)): 
    print(matrix[i])

def DijkstraAlgorithm(matrix,V,E):
    location = 0
    list_visted = []
